{
  "name": "mandarinko",
  "version": "0.0.1",
  "description": "Chinese and Japanese Vocabulary Building",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "pnpm --recursive --parallel run dev",
    "build": "pnpm --recursive run build",
    "test": "pnpm --recursive run test",
    "test:ui": "vitest --run --ui",
    "test:coverage": "vitest --run --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "pnpm --recursive exec rm -rf dist"
  },
  "keywords": [
    "chinese",
    "japanese",
    "vocabulary",
    "hsk",
    "jlpt",
    "kanji",
    "characters"
  ],
  "author": "SQLCODE917",
  "license": "GPL-3.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
