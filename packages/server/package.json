{
  "name": "@mandarinko/server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "exports": {
    ".": {
      "import": "./dist/cli.js",
      "default": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    },
    "./types": {
      "import": "./dist/apiTypes.js",
      "default": "./dist/apiTypes.js",
      "types": "./dist/apiTypes.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b && node dist/cli.js",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@mandarinko/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "vitest": "^1.1.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12"
  }
}
